{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://task-manager-backend-bay.vercel.app"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
